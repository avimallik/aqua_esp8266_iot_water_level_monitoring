\begin{document}

% Distance calculation in centimeters
\[
\text{distance}_{\text{cm}} = \frac{\text{duration} \times v_{\text{sound}}}{2 \times 10^{4}}
\]

% Distance conversion to inches
\[
\text{distance}_{\text{inch}} = \text{distance}_{\text{cm}} \times 0.393701
\]

% Water level percentage calculation
\[
\text{percentage} = 100 \times \left(1 - \frac{\text{distance}_{\text{cm}}}{\text{tankHeight}}\right)
\]

